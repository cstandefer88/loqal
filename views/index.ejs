<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>loqal</title>
    <meta name="viewport" content="initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>

    <h1>loqal</h1>
    <div id="container">
      <div id="app-container">
      </div>
      <div id="map"></div>
    </div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -34.397, lng: 150.644},
          zoom: 8
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAWHNtqn_uj78l92XOMkLgSI7AODat6Ums&callback=initMap"
    async defer></script>
    <script src="/javascripts/bundle.js"></script>
  </body>
</html>
